<form method="post" action="{{ path('app_alumnos_delete', {'id': alumno.id}) }}" onsubmit="return confirm('¿Estás seguro de eliminarlo?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ alumno.id) }}">
    <button button style="background-color:red" class="btn btn-primary">Delete</button>
</form>
